{
  "name": "bunce-node",
  "version": "0.0.1",
  "description": "Node SDK for Bunce APIs",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "typings": "dist/cjs/index.d.ts",
  "packageManager": "pnpm@9.13.2",
  "private": "false",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist/**",
    "!**/*.spec.*",
    "!**/*.json",
    "CHANGELOG.md",
    "LICENSE",
    "README.md"
  ],
  "engines": {
    "node": ">=18.16.1"
  },
  "engineStrict": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/trybunce/bunce-node.git"
  },
  "keywords": [
    "Bunce",
    "API",
    "Node",
    "SDK",
    "Customer Engagement"
  ],
  "author": "Damilare Anjorin",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/trybunce/bunce-node/issues"
  },
  "homepage": "https://github.com/trybunce/bunce-node#readme",
  "scripts": {
    "build": "pnpm run '/^build:.*/'",
    "build:main": "tsc -p tsconfig.json",
    "build:module": "tsc -p tsconfig.module.json",
    "format": "prettier --write --cache \"./**/*.{js,ts,md}\"",
    "format:check": "prettier --check \"./**/*.{js,ts,md}\"",
    "lint": "eslint --cache .",
    "lint:fix": "eslint . --cache --fix",
    "postinstall": "simple-git-hooks",
    "typecheck": "tsc --noEmit",
    "clean": "rimraf 'node_modules' 'dist'",
    "test": "vitest run",
    "test:watch": "vitest --watch",
    "test:debug": "vitest --inspect-brk --inspect --logHeapUsage --threads=false",
    "release": "bumpp package.json --commit --push --tag"
  },
  "dependencies": {
    "axios": "^1.6.5"
  },
  "devDependencies": {
    "@damilaredev/eslint-config": "^0.0.19",
    "@types/node": "^20.11.5",
    "@vitest/coverage-v8": "^2.0.0",
    "bumpp": "^9.3.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.2",
    "lint-staged": "^15.2.0",
    "prettier": "^3.2.4",
    "rimraf": "^6.0.0",
    "simple-git-hooks": "^2.9.0",
    "typescript": "^5.3.3",
    "vitest": "^2.0.0"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  }
}
